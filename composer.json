{
  "name": "lts/dsm-runtime",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "doctrine/data-fixtures": "@stable",
    "doctrine/dbal": "@stable",
    "doctrine/migrations": "@stable",
    "doctrine/orm": "@stable",
    "edmondscommerce/typesafe-functions": "dev-PHP8@dev",
    "moneyphp/money": "^3.3.1",
    "ramsey/uuid-doctrine": "@stable",
    "symfony/cache": "@stable",
    "symfony/config": "@stable",
    "symfony/console": "@stable",
    "symfony/dependency-injection": "@stable",
    "symfony/filesystem": "@stable",
    "symfony/intl": "@stable",
    "symfony/validator": "@stable",
    "ext-filter": "*",
    "ramsey/uuid": "@stable",
    "fakerphp/faker": "^1.20",
    "doctrine/common": "@stable",
    "doctrine/persistence": "@stable",
    "doctrine/inflector": "@stable"
  },
  "require-dev": {
    "lts/php-qa-ci": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "LTS\\DsmRuntime\\": [
        "src/"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true
    }
  }
}
